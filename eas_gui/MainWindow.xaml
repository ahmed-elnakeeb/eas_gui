            <TabItem HorizontalAlignment="Center" Height="20" Header="exams" VerticalAlignment="Center" Width="70">
                <Grid Background="#FFE5E5E5">
                    <Button x:Name="button_delete" Content="delete" HorizontalAlignment="Left" Width="50" Margin="242,67,0,0" VerticalAlignment="Top" Click="button_delete_Click"/>
                    <DataGrid x:Name="dataGrid" HorizontalAlignment="Left" Height="140" Margin="10,17,0,0" VerticalAlignment="Top" Width="215"  AutoGenerateColumns="False" SelectedCellsChanged="dataGrid_SelectedCellsChanged">

                        <DataGrid.Columns>

                            <DataGridTextColumn Header="Name" Binding="{Binding Name}" />
                            <!--<DataGridTextColumn Header="Question" Binding="{Binding Questions}" />-->


                        </DataGrid.Columns>
                        <DataGrid.RowDetailsTemplate>
                            <DataTemplate>
                                <TextBlock Text="{Binding Details}"  />
                            </DataTemplate>
                        </DataGrid.RowDetailsTemplate>

                    </DataGrid>
                    <DataGrid x:Name="dataGrid1" HorizontalAlignment="Left" Height="185" Margin="10,191,0,0" VerticalAlignment="Top" Width="215" >

                        <DataGrid.Columns>
                            <DataGridTextColumn Header="Question" Binding="{Binding Q}" />
                            <!--<DataGridTextColumn Header="Answer" Binding="{Binding A}" />-->
                        </DataGrid.Columns>
                    </DataGrid>
                    <Button x:Name="button_from_csv" Content="from csv" HorizontalAlignment="Left" Width="50" Margin="242,42,0,0" VerticalAlignment="Top" Click="button_from_csv_Click"/>
                </Grid>
            </TabItem>
